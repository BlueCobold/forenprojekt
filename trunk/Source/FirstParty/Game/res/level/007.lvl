            w4d3d5w2                  w1                   6        w3d2d4w5        
            w1d5d4w5    TT        TT  w3    TT        TT  w5    TT  w4d3d5w1        
            w4d1d3w6    ti            w2                  w1        w5d1d6w2        
                                                                                    
                                                                                    
                                                                                    
                                                                                    
                                                                                    
                                                                                    
                                                                                    
                                                                                    
	</grid>
	<objects>
		<include file="pastureland/background.inc" />
