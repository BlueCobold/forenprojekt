<level>
	<constants solid="0" />
	<gameplay maxBalls="5" time="180"/>
	<templates>
		<include file="assets/ball/twirl.inc" />
		<include file="assets/target/static.inc" />
		<include file="assets/teeter/wood.inc" />
		<include file="sandy/walls.inc" />

		<shapes>
			<shape name="checkpointArea" type="polygon">
				<vertex x="-120" y="-120" />
				<vertex x="420" y="-120" />
				<vertex x="420" y="0" />
				<vertex x="-120" y="0" />
			</shape>
		</shapes>

		<entities>
			<entity name="respawnLoc1" rep="R1" collideWithBall="false">
				<physics name="wall" shape="checkpointArea" />
				<collides>
					<copySpawnLocation>
						<always />
					</copySpawnLocation>
				</collides>
			</entity>

			<entity name="timer" rep="ti" collideWithBall="false">
				<physics name="timer" shape="teeter">
					<body type="kinematic" angle="0">
						<kinematics>
							<position axis="x">
								<setVar name="solid">
									<clamp>
										<int>
											<sub>
												<mod>
													<time />
													<static value="4" />
												</mod>
												<static value="1" />
											</sub>
										</int>
										<static value="0" />
										<static value="1" />
									</clamp>
								</setVar>
							</position>
						</kinematics>
					</body>
					<fixture density="0.5" friction="0.8" restitution="0.1" />
				</physics>
			</entity>

			<entity name="GhostTeeter" rep="T2" type="teeter">
				<physics name="teeter" shape="teeter" />
				<sound name="bounce" />
				<collides>
					<propertyFilter target="ball">
						<sub>
							<static value="1" />
							<var name="global:solid" />
						</sub>
					</propertyFilter>
				</collides>
				<animations>
					<animation texture="teeter_wooden" width="208" height="32" midx="104" midy="16" rotate="true" alignment="vertical">
						<frameindex frames="7">
							<min>
								<mul>
									<add>
										<step>
											<sub>
												<angle />
												<static value="180" />
											</sub>
											<angle />
											<sub>
												<max>
													<angle />
													<static value="315" />
												</max>
												<static value="360" />
											</sub>
										</step>
										<static value="45" />
									</add>
									<inv>
										<static value="90"/>
									</inv>
									<static value="7"/>
								</mul>
								<static value="6"/>
							</min>
						</frameindex>
						<color channel="alpha">
							<sub>
								<static value="1" />
								<mul>
									<var name="solid" />
									<static value="0.5" />
								</mul>
							</sub>
						</color>
					</animation>
				</animations>
			</entity>

		</entities>
	</templates>

	<grid size="60" width="2460" height="1650">
                                                                                    
                                                                                    
                                                                                    
                                                                                    
                                                                                    
                                                                                    
                                                                                    
                                                                                    
                                                                                    
