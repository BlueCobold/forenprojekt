<!-- Definition -->
<level>
 <objects>
  <entity name="string" rep="char" size="unsigned int" orientation="square OR vertical OR horizontal">
   <physics type="static OR kinematic OR dynamic" angle="float" fixedRotation="true OR false" linearVelocityX="float" linearVelocityY="float" linearDamping="float" angularVelocity="float" angularDamping="float">
    <shape type="circle OR polygon" num="unsigned int">
	 <vertex x="float" y="float" />
	 <vertex x="float" y="float" />
	 <vertex x="float" y="float" />
	 <vertex x="float" y="float" />
	</shape>
   </physics>
   <texture file="string" width="unsigned int" height="unsigned int" />
   <animation infinite="true OR false" min="float" step="float" num="unsigned int" width="unsigned int" height="unsigned int" />
  </entity>
 </objects>
 <grid width="unsigned int" height="unsigned int" size="unsigned int">
  char[][]
 </grid>
</level>

