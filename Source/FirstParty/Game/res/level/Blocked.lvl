					<position axis="y">
						<sub>
							<static value="73" />
							<mod>
								<mul>
									<time />
									<static value="60" />
								</mul>
								<static value="60" />
							</mod>
						</sub>
						
					</position>
					<position axis="x">
						<static value="13" />
					</position>
				</animation>
				<animation texture="gravity_arrow" width="36" height="36" srcy="128" srcx="0" >
					<frameindex frames="1">
						<static value="3" />
					</frameindex>
					<position axis="y">
						<sub>
							<static value="133" />
							<mod>
								<mul>
									<time />
									<static value="60" />
								</mul>
								<static value="60" />
							</mod>
						</sub>
						
					</position>
					<position axis="x">
						<static value="13" />
					</position>
					<color channel="alpha">
						<sub>
							<static value="1" />
							<mod>
								<mul>
									<time />
									<inv>
										<static value="1" />
									</inv>
								</mul>
								<static value="1" />
							</mod>
						</sub>
					</color>
				</animation>
			</animations>
		</entity>
		<entity name="target_special" rep="ts" type="target" respawnable="true" points="100" collideWithBall="false">
			<physics name="specialtarget">
				<shape type="circle" radius="31" />
			</physics>
			<collides>
				<spawnEntity name="target_2">
					<spawnEntity name="target_particles">
						<always />
					</spawnEntity>
				</spawnEntity>
			</collides>
			<animations>
				<animation texture="target" width="67" height="67" midx="33.5" midy="33.5" rotate="true" />
			</animations>
			<joints>
				<singlePrismatic x="33.5" y="33.5" directionX="0" directionY="-1" endPointBackward="0" endPointForward="300">
					<motor maxForce="0" speed="0" />
				</singlePrismatic>
			</joints>
		</entity>
		</entities>
		<functions>
			<function name="blockAlpha">
				<step>
					<var name="collides" />
					<step threshold="0">
						<setVar name="delta">
							<sub>
								<var name="killTime" />
								<mul>
									<time />
									<static value="2"/>
								</mul>
							</sub>
						</setVar>
						<stop />
						<var name="delta" />
					</step>
					<static value="1" />
				</step>
			</function>
			<function name="blockCollides">
				<nop>
					<static value="0" />
					<setVar name="killTime">
						<add>
							<mul>
								<time />
								<static value="2"/>
							</mul>
							<static value="1"/>
						</add>
					</setVar>
				</nop>
			</function>
		</functions>
		
		<overrides>
			<override rep="w*" newRep="k*">
				<constants collides="1" killTime="0" delta="0" />
				<animations>
					<color channel="alpha">
						<function name="blockAlpha"/>
					</color>
				</animations>
				<collides>
					<propertyFilter target="entity">
						<var name="collides" />
					</propertyFilter>
				</collides>
				<onCollision>
					<changeProperty name="collides">
						<function name="blockCollides"/>
					</changeProperty>
				</onCollision>
			</override>
		</overrides>
	</templates>
