		</entity>		
		
		<entity name="info2" rep="i2">
			<animations>
				<animation texture="gravity_arrow" width="36" height="36" srcy="128" srcx="0" >
					<frameindex frames="1">
						<static value="3" />
					</frameindex>
					<position axis="y">
						<sub>
							<static value="13" />
							<mod>
								<mul>
									<time />
									<static value="60" />
								</mul>
								<static value="60" />
							</mod>
						</sub>
					</position>
					<position axis="x">
						<static value="13" />
					</position>
					<color channel="alpha">
						<mod>
							<mul>
								<time />
								<inv>
									<static value="1" />
								</inv>
							</mul>
							<static value="1" />
						</mod>
					</color>
				</animation>
				<animation texture="gravity_arrow" width="36" height="36" srcy="128" srcx="0" >
					<frameindex frames="1">
						<static value="3" />
