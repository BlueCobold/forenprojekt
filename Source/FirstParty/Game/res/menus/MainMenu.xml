									<static value="0" /><!-- c -->
									<static value="0" /><!-- q -->
									<static value="0" /><!-- u -->
									<static value="0" /><!-- e -->
									<static value="-5" /><!-- t -->
									<static value="0" /><!-- * -->
								</switch>
								<mul><!-- center the lower row in relation to the upper -->
									<static value="-8" />
									<int>
										<mul>
											<mod>
												<variable name="orderId"/>
												<static value="14" />
											</mod>
											<static value="0.142857143" />
										</mul>
									</int>
								</mul>
								<mul><!-- shift each letter -->
									<mod>
										<variable name="orderId"/>
										<static value="7" />
									</mod>
									<add>
										<static value="20"/>
