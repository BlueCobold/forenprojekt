									<static value="0" /><!-- a -->
									<static value="-1" /><!-- c -->
									<static value="4" /><!-- q -->
									<static value="1" /><!-- u -->
									<static value="0" /><!-- e -->
									<static value="-6" /><!-- t -->
									<static value="0" /><!-- * -->
								</switch>
								</add>
							</cache>
							<mul>
								<pow>
									<abs>
										<sine>
											<mul>
												<sub>
													<cache>
														<mul>
															<mod>
																<variable name="orderId"/>
																<static value="14" />
															</mod>
															<static value="-10"/>
														</mul>
													</cache>
													<time />
												</sub>
												<static value="220"/>
											</mul>
										</sine>
									</abs>
									<static value="0.7" />
								</pow>
								<static value="-2.5" />
							</mul>
						</add>
					</position>
				</animation>
